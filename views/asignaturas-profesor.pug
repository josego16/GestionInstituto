extends layout

block content
    h1 Asignaturas matriculadas
    h2 Profesor: #{profesor.nombre} #{profesor.apelllidos}
    if asignaturasProfesores.length > 0
        ul
            each asignatura in asignaturasProfesores
                li= asignatura.asignatura
    else
        p El profesor no esta matriculado en ninguna asignatura.
    a(href='/matricularPf') Volver atras